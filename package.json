{
  "name": "vat-deductor-tmp-main",
  "version": "1.0.0",
  "description": "MVP tool aimed at shortcutting a vendor-buyer process related to the current Venezuelan tax system.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node --watch --env-file=.env --experimental-strip-types --experimental-sqlite ./server/server.js",
    "client": "npm --prefix client run dev",
    "test": "vitest",
    "postinstall": "prisma generate --no-engine"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.16",
    "@tippyjs/react": "^4.2.6",
    "bcryptjs": "^3.0.2",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "ldrs": "^1.1.9",
    "motion": "^12.23.24",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "sqlite3": "^5.1.7",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
